    "editor.fontSize": 15,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.tabSize": 4,
    "explorer.autoReveal": true,
    "explorer.confirmDelete": false,
    "explorer.sortOrder": "type",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.mustache": "mustache",
        "*.rmd": "markdown"
    },
    "files.autoSave": "off",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "git.autofetch": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.keymap": "none",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.experiments.enabled": false,
    "jupyter.interactiveWindowMode": "perFile",
    "latex-workshop.view.pdf.viewer": "tab",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "macros": {
        "pythonExecSelectionAndCursorDown": [
            "python.execSelectionInTerminal",
            "cursorDown"
        ]
    },
    "material-icon-theme.folders.theme": "classic",
    "multiCommand.commands": [
        {
            "command": "multiCommand.toggleCommentAndDown",
            "sequence": [
                "editor.action.commentLine",
                "cursorDown"
            ]
        },
        {
            "command": "multiCommand.closeAllPanels",
            "sequence": [
                "workbench.action.closePanel",
                "workbench.action.maximizeEditor"
            ]
        }
    ],
    "python.defaultInterpreterPath": "/bin/python",
    "python.languageServer": "Pylance",
    "python.terminal.activateEnvironment": false,
    "r.sessionWatcher": true,
    "redhat.telemetry.enabled": false,
    "rust-analyzer.procMacro.enable": true,
    "rust-client.channel": "stable",
    "rust-client.engine": "rust-analyzer",
    "typescript.updateImportsOnFileMove.enabled": "never",
    "update.mode": "none",
    "vs-kubernetes": {
        "checkForMinikubeUpgrade": true,
        "imageBuildTool": "Docker",
        "logsDisplay": "webview",
        "vs-kubernetes.autoCleanupOnDebugTerminate": false,
        "vs-kubernetes.draft-path": "",
        "vs-kubernetes.helm-path": "",
        "vs-kubernetes.knownKubeconfigs": [],
        "vs-kubernetes.kubeconfig": "",
        "vs-kubernetes.kubectl-path": "",
        "vs-kubernetes.kubectlVersioning": "user-provided",
        "vs-kubernetes.minikube-path": "",
        "vs-kubernetes.namespace": "",
        "vs-kubernetes.nodejs-autodetect-remote-root": true,
        "vs-kubernetes.nodejs-debug-port": 9229,
        "vs-kubernetes.nodejs-remote-root": "",
        "vs-kubernetes.outputFormat": "yaml"
    },
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "toggle",
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.guides.indentation": true,
    "python.formatting.blackArgs": [
        "--line-length",
        "79",
        "--skip-string-normalization"
    ],
    "python.formatting.provider": "black",
    "editor.rulers": [
        {
            "column": 79
        }
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.detectIndentation": false,
